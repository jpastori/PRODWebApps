			</CFIF>
			<CFIF FORM.BLD_SIGNAGE EQ 'YES'>
				LQC.BLD_SIGNAGE = 'YES' #LOGICANDOR#
			</CFIF>
			<CFIF FORM.BLD_STUDYSPACE EQ 'YES'>

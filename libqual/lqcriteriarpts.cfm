			</CFIF>
			<CFIF FORM.POL_NOISE EQ 'YES'>
				LQC.POL_NOISE = 'YES' #LOGICANDOR#
			</CFIF>
			<CFIF FORM.POL_SLEEPING EQ 'YES'>
				LQC.POL_SLEEPING = 'YES' #LOGICANDOR#
			</CFIF>

			<CFIF FORM.REPORTGROUP EQ 'YES'>

			</CFIF>
			<CFIF FORM.TECH_REMOTEACCESS EQ 'YES'>
				LQC.TECH_REMOTEACCESS = 'YES' #LOGICANDOR#
			</CFIF>
			<CFIF FORM.TECH_RBRLAB EQ 'YES'>
				LQC.TECH_RBRLAB = 'YES' #LOGICANDOR#
			</CFIF>
			<CFIF FORM.TECH_SCCLAB EQ 'YES'>
				LQC.TECH_SCCLAB = 'YES' #LOGICANDOR#
			</CFIF>

				#LOOKUPFIELD# = 'YES' AND
				LQC.COMMENTID #FINALTEST# 0)
		ORDER BY	#REPORTORDER#
	</cfquery>

	<CFSET RECCOUNT = #RECCOUNT# + #SelectLIBQUAL_Comments.RecordCount#>
	<CFSET COMPAREHEADER1 = #Evaluate("#FIELDNAME1#")#>
	<CFIF REPORTHEADER1 NEQ #COMPAREHEADER1#>
		<CFSET REPORTHEADER1 = #COMPAREHEADER1#>
		<tr>
			<td ALIGN="CENTER" COLSPAN="6"><HR SIZE="5" NOSHADE /></td>
		</tr>
		<TR>
			<TH align="left"><H2>#REPORTHEADER1#</H2></TH>
		</TR>
	</CFIF>
	<CFSET COMPAREHEADER2 = #Evaluate("#FIELDNAME2#")#>
	<CFIF REPORTHEADER2 NEQ #COMPAREHEADER2#>
		<CFSET REPORTHEADER2 = #COMPAREHEADER2#>
	</CFIF>

	<cfloop query = "SelectLIBQUAL_Comments">

		<tr>
			<td ALIGN="left"><DIV>#REPORTHEADER2#</DIV></td>
			<td ALIGN="left"><DIV>#SelectLIBQUAL_Comments.POSITIONNAME#</DIV></td>
			<td ALIGN="CENTER" nowrap><DIV>#SelectLIBQUAL_Comments.AGERANGENAME#</DIV></td>
			<td ALIGN="CENTER" nowrap><DIV>#SelectLIBQUAL_Comments.GENDERNAME#</DIV></td>
			<td ALIGN="left" nowrap><DIV>#SelectLIBQUAL_Comments.DISCIPLINENAME#</DIV></td>
			<td ALIGN="CENTER" nowrap><DIV>#SelectLIBQUAL_Comments.LIBQUALRECID#</DIV></td>
		</TR>
		<TR>
			<td ALIGN="left" COLSPAN="6"><DIV>#SelectLIBQUAL_Comments.COMMENTS#</DIV></td>
		</TR>
		<tr>
			<td ALIGN="CENTER" COLSPAN="6"><HR /></td>
		</tr>

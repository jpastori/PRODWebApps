			<TH ALIGN="CENTER" VALIGN="BOTTOM">Age Range</TH>
			<TH ALIGN="CENTER" VALIGN="BOTTOM">Gender</TH>
			<TH ALIGN="CENTER">Libqual Record ID</TH>
			<TH ALIGN="left" >&nbsp;&nbsp;</TH>
		</tr>
<cfloop query = "ListLIBQUAL_Comments">
